"problemMatcher": {
    "owner": "c",
    "fileLocation": [
        "relative",
        "${workspaceFolder}/"
    ],
    "pattern": {
        "regexp": "^\"(.+)\", line ([0-9]+): (.+) [(].+:.+[)]: (.+)$",
        "file": 1,
        "line": 2,
        //"column": 3,
        "severity": 3,
        "message": 4
    }
}
